reset
set size 0.8,0.5
set terminal postscript enhanced color lw 1.5 "Helvetica" 13
set out "sense_ver_times.ps"
set autoscale
set key left Left reverse
unset log
unset label
set style line  1 lt 1 lw 2
set style line  2 lt 2 lw 2
set style line  3 lt 3 lw 2
set style line  4 lt 4 lw 2
set style line  5 lt 6 lw 2
set xtic auto
set ytic auto
set title "Sense"
set xlabel "Claim"
set ylabel "Time (minutes)"
#set xrange [0950:1650]
plot \
     "test.txt" using xticlabels(1):2 w filledcurve x1 ls 1 title 'Total verification time', \
     "test.txt" using xticlabels(1):($2-$3) w filledcurve x1 ls 2 title 'Dec. proc. runtime'
