reset
set size ratio 1.75
set terminal postscript color lw 1.5 "Helvetica, 10"
set out "sense_times3.ps"
set autoscale
set key left Left reverse
unset log
unset label
set style line  1 lt 1 lw 5
set style line  2 lt 2 lw 1
set style line  3 lt 3 lw 1
set style line  4 lt 4 lw 5
set style line  5 lt 5 lw 1
set style line  6 lt 6 lw 1
#set xtic auto
set xtics font "Helvetica, 10"
set xtics rotate 90
set ytics rotate 90
set ytic auto
set ylabel "Verification time (minutes) per assertion"
set yrange [0:69]
plot \
     "sense.txt" using ($4/60):xticlabels(6) \
        with filledcurve x1 ls 2 \
        title 'Decision procedure, 2 IRQs', \
     "sense.txt" using (($4-$5)/60):xticlabels(6) \
        with filledcurve x1 ls 3 \
        title 'Program unwinding, 2 IRQs', \
     "sense.txt" using ($2/60):xticlabels(6) \
        with filledcurve x1 ls 5 \
        title 'Decision procedure', \
     "sense.txt" using (($2-$3)/60):xticlabels(6) \
        with filledcurve x1 ls 6 \
        title 'Program unwinding'

#     "sense.txt" using ($4/60):xticlabels(6) \
#        with lines ls 1 \
#        title 'Total verification time 2', \

