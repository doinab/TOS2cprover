#!/bin/bash

START=$(date +%s)

cbmc "$1" \
  --16 \
  --unwind 1 \
  --unwindset \
0:17,1:17,2:2,3:3,4:5,5:5,6:2,7:17,9:3,10:5,11:5,12:17,14:3,17:5,18:2,19:5,20:4,21:5,22:5,23:5,24:3,25:9,26:2,27:5,28:5,29:3,30:3,31:3,32:3,33:5,34:3,35:5,36:5,37:2,38:2,39:2,40:3,41:5,42:5,43:17,45:3,46:5,47:4,48:9,49:2,50:3,51:3,52:2,54:5,55:3,56:5,57:5,58:3,59:5,60:5,61:4,62:5,63:5,65:3,66:3,67:3,68:3 \
  --no-unwinding-assertions

END=$(date +%s)
DIFF=$(( $END - $START ))

echo "Verification time: $DIFF seconds"

