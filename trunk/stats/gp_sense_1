reset
set size ratio 0.24
set lmargin 1
set rmargin 1
set terminal postscript color lw 1.5 "Helvetica, 11"
set out "sense_times1.ps"
set autoscale
set key left Left reverse
unset log
unset label
set style line  1 lt 1 lw 5
set style line  2 lt 2 lw 1
set style line  3 lt 3 lw 1
set style line  4 lt 4 lw 1
set style line  5 lt 6 lw 1
#set xtic auto
set xtics font "Helvetica, 10"
set xtics rotate 90
set ytic auto
set title "Sense; one TimerB interrupt and one scheduler loop unwinding"
set xlabel "Claim" 1,4
set ylabel "Verification time (minutes)"
set yrange [1:6]
plot \
     "sense.txt" using ($4/60):xticlabels(6) \
        with lines ls 1 \
        title 'Total verification time 2', \
     "sense.txt" using ($4/60):xticlabels(6) \
        with filledcurve x1 ls 2 \
        title 'Decision procedure runtime 2', \
     "sense.txt" using (($4-$5)/60):xticlabels(6) \
        with filledcurve x1 ls 3 \
        title 'Program unwinding time 2', \
     "sense.txt" using ($2/60):xticlabels(6) \
        with lines ls 1 \
        title 'Total verification time', \
     "sense.txt" using ($2/60):xticlabels(6) \
        with filledcurve x1 ls 2 \
        title 'Decision procedure runtime', \
     "sense.txt" using (($2-$3)/60):xticlabels(6) \
        with filledcurve x1 ls 3 \
        title 'Program unwinding time'
